{
  "name": "CMS User Service",
  "version": 2,
  "env": {
    "GOOGLE_APPLICATION_CREDENTIALS": "@firebaseserviceaccount"
  },
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "access-control-allow-origin": "*",
        "access-control-allow-methods": "PUT",
        "access-control-allow-headers": "content-type"
      },
      "methods": [
        "PUT",
        "OPTIONS",
        "POST",
        "GET"
      ],
      "dest": "/api/$1.js"
    }
  ]
}